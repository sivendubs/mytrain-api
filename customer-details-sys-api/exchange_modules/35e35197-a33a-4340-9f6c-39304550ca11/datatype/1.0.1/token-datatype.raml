#%RAML 1.0 DataType

type: object
properties:
  type?: 
    description: Type of token
    type: string
    example: "JWT"

  expirationPeriod:
    description: Indicate how long the token is active
    type: integer
    example: 10

  token:
    description: Actual token value
    type: string
    example: "eb900942FcB74a0482Db02A881b2b23C"